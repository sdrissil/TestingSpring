<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<bean id="student" class="com.spring.test.Student">
		<property name="name" value="a student has no name" />
		<property name="age" value="a student has no age" />
	</bean>

	<bean id="spellChecker" class="com.spring.test.SpellChecker">
	</bean>

	<bean id="textEditor" class="com.spring.test.TextEditor">
	</bean>
</beans>